# ===========================================
# ⚠️ FICHIER .env COMPLET (DUCKDNS + ALL SERVICES)
# ===========================================

# --- Database ---
POSTGRES_USER=postgres
POSTGRES_PASSWORD=123456
POSTGRES_DB=digital_logistics
POSTGRES_PORT=5433
DB_PASSWORD=123456

# --- PgAdmin ---
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=123456
PGADMIN_PORT=8081

# --- Application ---
SERVER_PORT=8093
APP_HOST_PORT=8093

# --- AWS / DuckDNS (MODIFIÉ ✅) ---
PUBLIC_HOST=logistics-prod-api.duckdns.org
KEYCLOAK_PORT=8080

# --- Keycloak ---
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_ISSUER_URI=http://logistics-prod-api.duckdns.org:8080/auth/realms/logistics-realm
KEYCLOAK_JWKS_HOST=keycloak:8080
KC_PROXY=edge
KC_HOSTNAME_STRICT_BACKCHANNEL=false

# --- Swagger ---
SWAGGER_SERVER_URL=http://logistics-prod-api.duckdns.org:8093

# --- Spring Datasource ---
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/digital_logistics
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=123456
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=false

# --- ELK Stack ---
ELASTICSEARCH_HOST=elasticsearch
ELASTICSEARCH_PORT=9200
LOGSTASH_HOST=logstash
LOGSTASH_PORT=5000
KIBANA_PORT=5601

# --- SonarQube (AJOUTÉ ✅) ---
SONAR_PORT=9001
SONAR_HOST=http://logistics-prod-api.duckdns.org:9001
SONAR_TOKEN=logistics_sonar_token_123

# --- Jenkins (AJOUTÉ ✅) ---
JENKINS_PORT=8089
JENKINS_AGENT_PORT=50000

# --- Docker Compose ---
COMPOSE_PROJECT_NAME=digitallogistics_prod