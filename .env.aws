# ===========================================
# Configuration AWS Production
# ===========================================
# IMPORTANT: Copier ce fichier en .env sur votre serveur AWS

# ===========================================
# Database Configuration
# ===========================================
POSTGRES_USER=postgres
POSTGRES_PASSWORD=123456
POSTGRES_DB=digital_logistics
POSTGRES_PORT=5433

# ===========================================
# PgAdmin Configuration
# ===========================================
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=123456
PGADMIN_PORT=8081

# ===========================================
# Application Configuration
# ===========================================
SERVER_PORT=8093
APP_HOST_PORT=8093

# ===========================================
# AWS / Production Configuration
# ===========================================
# IMPORTANT: Remplacez cette IP par votre IP publique AWS
PUBLIC_HOST=13.60.41.134
KEYCLOAK_PORT=8080

# URLs Keycloak pour la production (accessible depuis Internet)
KEYCLOAK_ISSUER_URI=http://13.60.41.134:8080/auth/realms/logistics-realm
KEYCLOAK_JWKS_HOST=keycloak:8080

# ===========================================
# Spring Datasource
# ===========================================
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/digital_logistics
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=123456
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=false
SPRING_JPA_DIALECT=org.hibernate.dialect.PostgreSQLDialect

# ===========================================
# SonarQube Configuration
# ===========================================
SONAR_PORT=9001
SONAR_HOST=http://localhost:9001
SONAR_TOKEN=

# ===========================================
# Jenkins Configuration
# ===========================================
JENKINS_PORT=8089
JENKINS_AGENT_PORT=50000

# ===========================================
# Docker Compose
# ===========================================
COMPOSE_PROJECT_NAME=digitallogistics_prod
